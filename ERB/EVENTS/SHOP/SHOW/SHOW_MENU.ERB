@showMenu
	; Target does not exist
	if !isMasterFree()
		setcolor 0x777777
	; 자궁대여중
	elseif FLAG:자궁대여료
		setcolor 0x777777
	endif
	printLC [101] - 미궁으로
	resetcolor

	; Target already exists
	if !isMasterFree() || hasQuest()
		setcolor 0x777777
	endif
	printLC [102] - 의뢰 수주
	resetcolor

	; Target does not exist
	if !isMasterFree() || !hasQuest()
		setcolor 0x777777
	endif
	printLC [103] - 의뢰 취소
	resetcolor

	if hasDeliverQuest()
		if !isMasterFree() || !hasDeliveryItems()
			setcolor 0x777777
		endif
		printLC [104] - 납품
		resetcolor
	endif
	printL

	printLC [201] - 상태창
	printLC [202] - 인벤토리
	if !isMasterFree()
		setColor 0x777777
	endif
	printLC [203] - 장비변경
	resetColor
	printL

	printLC [204] - 스킬창
	resetColor
	printL

	if !isMasterFree()
		setColor 0x777777
	endif
	printLC [301] - 상가
	printLC [302] - 뒷골목
	printLC [303] - 성당
	resetColor
	printL
	
	printL

	printLC [801] - 저장
	printLC [802] - 불러오기
	printLC [803] - CONFIG
	printL
	
	printLC [900] - 눈을 붙인다
	if !isMasterFree() || MONEY < 2000
		setColor 0x777777
	endif
	printLC [901] - 푹 쉰다(\2000 소모)
	resetColor
