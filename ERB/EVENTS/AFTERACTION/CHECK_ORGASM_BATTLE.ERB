@checkOrgasmInTown
#dim dynamic orgasmThres
#dim dynamic orgasmCount
#dim dynamic orgasmParts, 100
#dim dynamic insuffOrgasm

	orgasmThres = masterOrgasmThres()
	for i, 501, 600
		if CFLAG:MASTER:i >= orgasmThres
			printFormW %STR:(i - 500)% 절정
			orgasmParts:(orgasmCount++) = i - 400
			DOWN:i = CFLAG:MASTER:i - CFLAG:MASTER:i % orgasmThres

			; 절정경험 증가
			EXP:MASTER:절정경험 += CFLAG:MASTER:i / orgasmThres
			EXP:MASTER:(i + 100) += CFLAG:MASTER:i / orgasmThres
		elseif CFLAG:MASTER:i >= 10000
			; 절정봉인 적용중
			insuffOrgasm++
		endif
	next

	if orgasmCount
		call onMasterOrgasm_m(orgasmParts, orgasmCount, 0)
	elseif insuffOrgasm
		printFormL %kr(CALLNAME:MASTER, "은", "는")% 절정을 봉인당한 상태다
		call onMasterInsuffOrgasm_m(0)
	endif
