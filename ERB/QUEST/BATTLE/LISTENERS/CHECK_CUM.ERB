@checkCum

	if PALAM:TARGET:사정 >= 10000
		if CFLAG:TARGET:생식분류 == 1
			;사정
		else
			;산란
		endif
		
		if ABL:MASTER:흡정
			printFormL %kr(NAME:TARGET, "은", "는")% %CALLNAME:MASTER%에게 착정당하고 있다

			; 최대 마력의 5%를 강탈
			if BASE:TARGET:마력 >= MAXBASE:TARGET:마력 / 20
				DOWNBASE:TARGET:마력 += MAXBASE:TARGET:마력 / 20
			; 무족하면 패널티
			; 최대 체력의 2%
			else
				printFormL 하지만 %CALLNAME:TARGET%의 마력은 이미 바닥이었고, 대신해서 체력을 흡수당했다

				DOWNBASE:TARGET:마력 = BASE:TARGET:마력
				DOWNBASE:TARGET:체력 = MAXBASE:TARGET:체력 / 50
			endif
		endif

		PALAM:TARGET:사정 -= 10000
	endif
