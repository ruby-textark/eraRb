@COM_ABLE102

	return isTargetInRange()

@COM102
#dim dynamic damageBase

	printW 몸통을 노렸다!
	
	if !checkAmmo() && !isSameLocation()
		printFormW 하지만 %kr(getAmmoName(), "이", "가")% 부족하여 실패했다...
		return 0
	endif
	
	damageBase = 10 * MAX(2, getActualAtk(MASTER) - getActualDef(TARGET))
	LOSEBASE:체력 = damageBase * 9 / 10 + RAND:(damageBase / 10)
	
	call useAmmo

	return 1
