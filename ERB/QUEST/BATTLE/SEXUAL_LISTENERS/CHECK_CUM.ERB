@checkCum
#dim dynamic cumAmount

	if TALENT:TARGET:여성
		return
	endif

	if PALAM:TARGET:사정 >= 10000
		cumAmount = 1 + RAND:2
		if TALENT:TARGET:대량사정 == 1
			cumAmount *= (2 + RAND:4)
		endif
		
		; 난소/자궁/질
		if TCVAR:TARGET:성희롱부위 == 115 || TCVAR:TARGET:성희롱부위 == 116 || TCVAR:TARGET:성희롱부위 == 121
			call cumIntoMaster(cumAmount, CFLAG:TARGET:종족)
		endif

		PALAM:TARGET:사정 -= 10000

		call onEnemyCum_m(TCVAR:TARGET:성희롱부위)
		call onEnemyCum_e(TCVAR:TARGET:성희롱부위)
	endif
