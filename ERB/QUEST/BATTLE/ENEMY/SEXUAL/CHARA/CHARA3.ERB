@chara3Parasite
#dim dynamic partsCount
#dim dynamic attackedIdx

	partsCount = 0
	for i, 301, 400
		if strlens(CFLAGNAME:i) && CFLAG:MASTER:i < 2 && isAttackableBodyPart(i - 200)
			partsCount++
		endif
	next
	
	attackedIdx = RAND:partsCount

	partsCount = 0
	for i, 301, 400
		if strlens(CFLAGNAME:i) && CFLAG:MASTER:i < 2 && isAttackableBodyPart(i - 200)
			if partsCount == attackedIdx
				; 기생
				; todo 구상추가
				printFormW %kr(CALLNAME:TARGET, "이", "가")% 꾸물거리며 %CALLNAME:MASTER%의 %bodyPartStr(i - 200)% 안으로 기어들어간다
				printFormW %kr(CALLNAME:MASTER, "은", "는")% [기생]상태가 되었다.
				CFLAG:MASTER:i = 3
				CFLAG:TARGET:기생성공 = 1

				return 1
			endif
			partsCount++
		endif
	next

	return 0

@chara3SexualAttack

	; 5%확률로 기생
	if RAND:20 == 0
		call chara3Parasite
		; 실패했을 경우 음침한 슬라임과 같음
		if !RESULT
			call chara2SexualAttack
		endif
	; 그 외의 경우 음침한 슬라임과 같음
	else
		call chara2SexualAttack
	endif
