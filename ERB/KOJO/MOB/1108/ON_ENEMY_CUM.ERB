@c1108onEnemyCum(partIdx)
#dim dynamic partIdx

	; before
	if RAND:2 == 0
		printFormL %kr(CALLNAME:MASTER, "은", "는")% 무언가 %NAME:TARGET%의 것이 조금 더 커진 듯한 느낌이 들었다
	else
		printFormL %kr(CALLNAME:MASTER, "은", "는")% %NAME:TARGET%의 움직임이 점점 빨라지는 것에서 사정의 전조를 느꼈다
	endif

	; on
	printFormL %CALLNAME:MASTER%의 안에 %NAME:TARGET%의 정액이 쏟아졌다

	; after
	printFormL %NAME:TARGET%의 자지가 크게 부풀어올라 %CALLNAME:MASTER%의 구멍을 틀어막았다
	printFormL %kr(NAME:TARGET, "은", "는")% 그렇게 한참을 버티다가 얼얼하다 못해 감각이 거의 느껴지지 않을 때가 돼서야 %kr(CALLNAME:MASTER, "을", "를")% 풀어주었다
