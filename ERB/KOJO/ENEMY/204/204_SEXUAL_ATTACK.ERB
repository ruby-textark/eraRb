			else
				printFormL %kr(CALLNAME:TARGET, "은", "는")% %CALLNAME:MASTER%의 %kr(partStr(attackedIdx), "을", "를")% 범했다
			endif
		; Tease
		else
			if RAND:3 == 0
				printFormL %kr(CALLNAME:TARGET, "은", "는")% %CALLNAME:MASTER%의 %kr(partStr(attackedIdx), "을", "를")% 아프게 꿰뚫었다
			elseif RAND:2 == 0
