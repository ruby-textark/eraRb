			printFormL %kr(CALLNAME:TARGET, "은", "는")% 손가락을 갖다 댈 때마다 움찔거리는 %CALLNAME:MASTER%의 반응을 즐기며 %kr(partStr(attackedIdx), "을", "를")% 마음껏 희롱하고 있다
		elseif RAND:3 == 0
			printFormL %kr(CALLNAME:TARGET, "은", "는")% %CALLNAME:MASTER%의 %partStr(attackedIdx)% 끝을 살살 약올리고 있다
		elseif RAND:2 == 0
			printFormL %kr(CALLNAME:TARGET, "은", "는")% %CALLNAME:MASTER%의 %kr(partStr(attackedIdx), "을", "를")% 살살 간지럽히고 있다
