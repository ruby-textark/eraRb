			printFormL %kr(CALLNAME:TARGET, "은", "는")% %CALLNAME:MASTER%의 %kr(partStr(attackedIdx), "을", "를")% 조금씩 꼬집으며 자극하고 있다
		endif
	else
		attackedIdx = getOrdinarySexualAtkBodyPart()

		; 쾌감상승
		if RAND:3 == 0
			printFormL %kr(CALLNAME:TARGET, "은", "는")% 붉은 마력을 해방했다
			printFormL %kr(CALLNAME:MASTER, "은", "는")% 몸 한구석에서 뜨거운 육욕을 느꼈다
			SOURCE:MASTER:attackedIdx += 3 * (90 + RAND:20)
		; 최면
		elseif RAND:2 == 0
			printFormL %kr(CALLNAME:TARGET, "은", "는")% 푸른 마력을 해방했다

			call startHypno
			
			if BASE:MASTER:최면심도 < 25
				BASE:MASTER:최면심도++
			else
