			else
				printFormL 하지만 %kr(CALLNAME:MASTER, "은", "는")% 저항해냈다
			endif
		endif

		attackedIdx = 114

		; 다만 자궁쾌감증가
		SOURCE:MASTER:116 += 90 + RAND:20
		return
	elseif attackedIdx == 124 || attackedIdx == 125
		; todo 방광요도구상
		; Already inserted
		if isMasterInserted(attackedIdx, 1)
