			elseif RAND:3 == 0
				printFormL %kr(CALLNAME:TARGET, "은", "는")% %CALLNAME:MASTER%의 %kr(partStr(attackedIdx), "을", "를")% 아프게 꿰뚫었다
			elseif RAND:2 == 0
				printFormL 오줌을 배출하던 %CALLNAME:MASTER%의 %kr(partStr(attackedIdx), "이", "가")% %CALLNAME:TARGET%에게 반대로 희롱당하며 평소에는 느낄 수 없던 쾌감을 주고 있다
			else
				printFormL %kr(CALLNAME:TARGET, "은", "는")% %CALLNAME:MASTER%의 %kr(partStr(attackedIdx), "을", "를")% 범했다
			endif
		endif
	elseif attackedIdx == 113
		if RAND:5 == 0 && MAXBASE:MASTER:모유 > 0
			printFormL %kr(CALLNAME:TARGET, "은", "는")% %CALLNAME:MASTER%의 %partStr(attackedIdx)%에 입을 대고 모유를 마시고 있다
