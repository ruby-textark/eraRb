			call colorPrint(0xcc0044, @"%kr(CALLNAME:TARGET, "은", "는")% %CALLNAME:MASTER%에게 음탕한 마력을 쏘아보냈다\n")

			if RAND:20 < ABL:TARGET:마법공격 - ABL:MASTER:마법내성
