@c102ClothDestroy
#dim dynamic breakableEquipSlots
#dim dynamic brokenEquipSlot

	breakableEquipSlots = 0
	for i, 11, 19
		if i != 16 && EQUIP:MASTER:i != 0
			breakableEquipSlots++
		endif
	next

	if breakableEquipSlots
		brokenEquipSlot = RAND:breakableEquipSlots
		breakableEquipSlots = 0

		for i, 11, 19
			if i != 16 && EQUIP:MASTER:i != 0
				if breakableEquipSlots == brokenEquipSlot
					printFormW %CALLNAME:TARGET%의 점액이 장비를 망가뜨린다
					printFormW 망가진 장비는 이번 전투 중에서는 사용할 수 없을 것 같다...
					call unequipMasterEquips(i)
				endif
				breakableEquipSlots++
			endif
		next
	endif


