@c103Parasite
#dim dynamic parasiteIdx
#dim dynamic partsCount
#dim dynamic attackedIdx
#dim dynamic availableParts, 100

	partsCount = 0
	for parasiteIdx, 101, 200
		if strlens(CFLAGNAME:parasiteIdx) && !isParasiteInside(parasiteIdx) && isAttackableBodyPart(parasiteIdx - 200)
			availableParts:(partsCount++) = parasiteIdx
		endif
	next
	
	attackedIdx = RAND:partsCount
	
	printFormW %kr(CALLNAME:TARGET, "이", "가")% 꾸물거리며 %CALLNAME:MASTER%의 %bodyPartStr((availableParts:attackedIdx) - 200)% 안으로 기어들어간다
	printFormW %kr(CALLNAME:MASTER, "은", "는")% [기생]상태가 되었다.
	call insertIntoMaster(attackedIdx, 3)
	CFLAG:TARGET:기생성공 = 1
	
	return 1

	return 0

@c103onSexualAttack

	; 5%확률로 기생
	if RAND:20 == 0
		call c103Parasite
		; 실패했을 경우 음침한 슬라임과 같음
		if !RESULT
			call c102onSexualAttack
		endif
	; 그 외의 경우 음침한 슬라임과 같음
	else
		call c102onSexualAttack
	endif
