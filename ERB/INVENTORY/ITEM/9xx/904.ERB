@isUsable904(targetIdx)
#dim dynamic targetIdx

	return targetIdx == MASTER

@onUseItem904(targetIdx)
#dim dynamic targetIdx

	printFormW %CALLNAME:targetIdx%에게 %kr(ITEMNAME:904, "을", "를")% 사용했다.
