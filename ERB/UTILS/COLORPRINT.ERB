@colorPrint(color, string)
#dim dynamic color
#dims dynamic string

	setColor color
	printForm %string%
	resetColor

	; RESULT sideeffect때문에
	return RESULT

@colorPrintW(color, string)
#dim dynamic color
#dims dynamic string

	setColor color
	if FLAG:전투중
		printFormL %string%
		twait 1000, 0
	else
		printFormW %string%
	endif
	resetColor

	; RESULT sideeffect때문에
	return RESULT
