@bodyPartStr(partIdx)
#functions
#dim dynamic partIdx
#dims dynamic retStr

	if partIdx < 101 || partIdx > 199
		printFormL {partIdx}는 올바른 신체 번호가 아닙니다
		returnf "ERROR"
	endif
	
	if RAND:3 == 0
		retStr += bodyPartPrefix(partIdx)
	elseif RAND:2 == 0
		retStr += specialPrefix(partIdx)
	endif
	
	returnf retStr + STR:partIdx

@bodyPartPrefix(partIdx)
#functions
#dim dynamic partIdx

	if RAND:3 == 0 && PALAM:MASTER:partIdx > 9000
		returnf "불타오르는 "
	elseif RAND:3 == 0 && PALAM:MASTER:partIdx > 8000
		returnf "뜨겁게 떨리는 "
	elseif RAND:3 == 0 && PALAM:MASTER:partIdx > 7000
		returnf "터질듯한 "
	elseif RAND:3 == 0 && PALAM:MASTER:partIdx > 6000
		returnf "흥분한 "
	elseif RAND:3 == 0 && PALAM:MASTER:partIdx > 4000
		returnf "오슬오슬한 "
	elseif RAND:3 == 0 && PALAM:MASTER:partIdx > 2000
		returnf "간질거리는 "
	else
		returnf ""
	endif

@specialPrefix(partIdx)
#functions
#dim dynamic partIdx

	if partIdx == 101
		if RAND:2 && ABL:MASTER:귀감도 > 9
			returnf "완전히 암컷구멍이 되어버리고 말아버린"
		elseif RAND:2 && ABL:MASTER:귀감도 > 6
			returnf "스치는 바람에도 느껴버리는"
		elseif RAND:2 && ABL:MASTER:귀감도 > 3
			returnf "성감대로 개발된"
		endif
	elseif partIdx == 102
		if RAND:2 && ABL:MASTER:입감도 > 9
			returnf "호흡할 때 조차 느껴버릴 정도로 개발된"
		elseif RAND:2 && ABL:MASTER:입감도 > 6
			returnf "어느새 음란한 구멍이 되어버린"
		elseif RAND:2 && ABL:MASTER:입감도 > 3
			returnf "성감대로 개발된"
		endif
	elseif partIdx == 111
	elseif partIdx == 112
	elseif partIdx == 113
	elseif partIdx == 114
	elseif partIdx == 115
	elseif partIdx == 116
	elseif partIdx == 121
	elseif partIdx == 122
	elseif partIdx == 123
	elseif partIdx == 124
	elseif partIdx == 125
	endif
