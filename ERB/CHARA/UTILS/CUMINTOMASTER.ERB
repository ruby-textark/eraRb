@cumIntoMaster(amount, fatherType)
#dim dynamic amount
#dim dynamic fatherType

	if TALENT:MASTER:피임결계
		printFormW %CALLNAME:MASTER%의 %kr(bodyPartStr(116), "은", "는")% 피임의 마법으로 보호되고 있다
		
		if BASE:MASTER:마력 < amount * 10
			if RAND:3 == 0
				printFormW 그러나 바닥나버린 마력에 의해 얇아진 방벽은 더 이상 자궁을 지켜줄 수 없었다
			elseif RAND:2 == 0
				printFormW 방심하고 있던 %kr(bodyPartStr(116), "이", "가")% 꿈틀거리던 찰나, 곧 바닥나버린 마력에 당황한 기색을 감추지 못했다
			else
				printFormW 하지만 밀려들어오는 정자를 막기에는 역부족이었다...
			endif

			printForm %CALLNAME:MASTER%의
			call colorPrint(0xff3399, "[피임결계]")
			printW 가 해제되었다
			
			TALENT:MASTER:피임결계 = 0
			
			if RAND:3 == 0
				printFormW 그동안 정액에 갈증을 느끼던 %CALLNAME:MASTER%의 %kr(bodyPartStr(116), "이", "가")% 무너진 결계 사이로 들어오는 정자에 환희하고 있다
			elseif RAND:2 == 0
				printFormW 얄팍한 마법 하나를 믿고 안심하던 %CALLNAME:MASTER%의 %bodyPartStr(116)%에 새하얀 국물이 따라진다
			else
				printFormW %CALLNAME:MASTER%의 %kr(bodyPartStr(116), "은", "는")% 밀물처럼 쏟아져 들어오는 정액을 막을 생각조차 못하고 그저 받아들이고 있다
			endif
		else
			BASE:MASTER:마력 -= amount * 10
			if BASE:MASTER:마력 < 100
				printFormW 
			endif
			return
		endif
	endif
	
	if TALENT:MASTER:교미배란
		printFormW %bodyPartStr(121)% 안으로 들어온 정액에 %CALLNAME:MASTER%의 %kr(bodyPartStr(115), "은", "는")% 환희하며 배란하고 있다.
		BASE:MASTER:생리주기 = MAXBASE:MASTER:생리주기 / 2
		TALENT:MASTER:생리중 = 0
		TALENT:MASTER:가임기 = 1
	endif

	BASE:MASTER:자궁내정액1 += amount
	BASE:MASTER:(320 + fatherType) += amount

	; 전투중일경우 필요함
	TFLAG:이번사정량 += amount
