@showTalents

	for i, 1, 1000
		; 모유체질
		if i == 5
			setColor 0xf5dcb7
		; 허벌보지
		elseif i == 10
			setColor 0x5e2c04
		; 세로애널
		elseif i == 10
			setColor 0x5e2c04
		; 음란
		elseif i == 10
			setColor 0xcc0000
		; 패배자
		elseif i == 11
			setColor 0xcc0000
		; 기생 / 이상성욕
		elseif 100 <= i && i <= 299
			fontBold
			setColor 0xcc0000
		; 임신
		elseif i == 401
			fontBold
			setColor 0xcc0000
		; 생리불순
		elseif i == 402
			fontBold
			setColor 0x3399ff
		; 가임기
		elseif i == 404
			fontBold
			setColor 0xff3399
		; 교미배란
		elseif i == 405
			fontBold
			setColor 0xff8c00
		endif

		if TALENT:MASTER:i
			printForm [%TALENTNAME:i%]
		endif
		
		fontRegular
		resetColor
	next
	printL
